extends ./base.jade

block title
  title Mama Connect

block content

  a(href=parent) ^ go up a level

  ul
    each option in menu
      li
        a(href=option.uri)= option.name

  p welcome to #{category.name}

  ul
    each medium in category.media
      li #{medium.name} (available #{medium.available})

  ul
    each site in category.sites
      li #{site.name}
